{
  "name": "@qorebase/client",
  "version": "2.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "test": "jest --verbose",
    "generate-schema": "openapi-typescript 'https://stg-qore-data.qore.one/documentation/json' --output ./src/schema.d.ts"
  },
  "devDependencies": {
    "@swc/core": "^1.3.42",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-mock-axios": "^4.7.1",
    "openapi-typescript": "^6.2.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "axios": "^1.3.4"
  },
  "tsup": {
    "target": "node16",
    "clean": true,
    "entryPoints": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true
  }
}
